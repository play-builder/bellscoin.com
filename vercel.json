{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "github": {
    "deployments": {
      "productionBranch": "main",
      "previews": {
        "halving": {
          "buildCommand": "",
          "outputDirectory": "."
        }
      }
    }
  }
}
